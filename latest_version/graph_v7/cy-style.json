        "style": {
            "edge-text-rotation": "autorotate",
            "width": 10
        }
    },
    {
        "selector": ".highlight-node",
        "style": {
          "border-style": "solid",
          "border-width": 10,
          "border-color": "#000",
          "opacity": 1
        }
    },
    {
      "selector": ".highlight-node-sec",
      "style": {
       
      }
  },

    {
